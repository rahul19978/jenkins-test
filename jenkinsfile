pipeline {
  agent any
  stages {
    stage('Clone') {
      steps {
        echo 'Cloning repo...'
      }
    }
    stage('Build') {
      steps {
        echo 'Building the project...'
      }
    }
    stage('Test') {
      steps {
        echo 'Running tests...'
      }
    }
    stage('Deploy') {
      steps {
        echo 'Deploying the application...'
      }
    }
  }
}

